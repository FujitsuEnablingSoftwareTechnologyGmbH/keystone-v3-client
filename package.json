{
    "name"           : "keystone-v3-client",
    "description"    : "A Node.js client for the OpenStack Keystone V3 Identity service",
    "version"        : "0.0.8",
    "keywords"       : [
        "keystone",
        "identity",
        "openstack",
        "v3",
        "client",
        "api"
    ],
    "author"         : "Fujitsu Enabling Software Technology GmbH",
    "engines"        : {
        "node": ">=0.10.37 <0.12",
        "npm" : "~2.10.1"
    },
    "repository"     : {
        "type": "git",
        "url" : "http://github.com/FujitsuEnablingSoftwareTechnologyGmbH/keystone-v3-client.git"
    },
    "main"           : "./index.js",
    "directories"    : {
        "lib"     : "./lib",
        "util"    : "./lib/util",
        "keystone": "./lib/keystone"
    },
    "dependencies"   : {
        "lodash"      : "~4.17.5",
        "debug"       : "~2.6.9",
        "bluebird"    : "2.9.x",
        "restler"     : "3.2.x",
        "memory-cache": "0.1.x"
    },
    "devDependencies": {
        "jshint"              : "2.7.x",
        "jshint-html-reporter": "0.2.x",
        "mocha"               : "2.2.x",
        "istanbul"            : "0.3.x",
        "mocha-istanbul"      : "0.2.0",
        "nock"                : "2.2.x",
        "proxyquire"          : "1.5.x",
        "should"              : "6.0.x",
        "sinon"               : "1.14.x",
        "jsdoc"               : "3.3.x"
    },
    "licenses": "Apache-2.0",
    "scripts"        : {
        "test"     : "./scripts/tests.sh",
        "lint"     : "./scripts/lint.sh",
        "lint:html": "./scripts/lint.html.sh",
        "coverage" : "./scripts/coverage.sh",
        "docs:code": "./scripts/documentation.sh"
    }
}
